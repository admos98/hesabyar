{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "GIST_ID": "@GIST_ID",
    "GITHUB_TOKEN": "@GITHUB_TOKEN",
    "GEMINI_API_KEY": "@GEMINI_API_KEY",
    "API_KEY": "@API_KEY"
  }
}
